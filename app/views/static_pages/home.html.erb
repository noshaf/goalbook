<div class="span12" align="center">
	<h1>Welcome to Twitter</h1>
	<br>
	<% if user_signed_in? %>
		<%= link_to image_tag("twitter.png") , tweets_path %>
	<% else %>
		<h1><%= link_to "Sign in", "/users/sign_in" %> </h1>
		<br>
		Or
		<br>
		<h1><%= link_to "Sign Up" , "/users/sign_up" %></h1>
	<% end %>
</div>
